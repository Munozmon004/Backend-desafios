const arr2 = []
const mostrar =(arreglo) => {
    if(arreglo.length === 0) {
        console.log(`La lista esta vacia`)
    } else {
        arreglo.forEach(element => {
            console.log(`${element}`)
        });
        console.log(`La longitud de la lista es de ${arreglo.length} `)
    }   
}
